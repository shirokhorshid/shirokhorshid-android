  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Zararli dastur harakati aniqlandi! Ko\'proq bilish uchun bosing.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon internetdagi potentsial xavfli saytlarga kirishni aniqladi va blok qiladi. Ko\'proq bilish uchun quyidagi havolalarni bosing.\n\nAniqlangan zararli dastur:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon internetdagi potentsial xavfli saytlarga kirishni avtomatik ravishda blok qiladi. Bu hodisa sodir bo\'lganda sizga xabar berilsinmi?\n\nBu sozlama keyinroq \"Ko\'proq imkoniyatlar\"da o\'zgartirilishi mumkin.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">MalAware haqida ko\'proq bilish uchun:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Yuqori proksi xatosi</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Psiphon-ni oching va proksi sozlamalarini tekshiring.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Proksi xatosi</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Siz Psiphon-ni yuqori proksi ishlatish uchun sozladingiz. Biroq, biz bu proksi orqali Psiphon serveriga ulanishda muammo bor.\n\nIltimos sozlamalarni to\'g\'irlang va qayta urinib ko\'ring.\n\nTexnik ma\'lumotlar uchun jurnallarga qarang.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon ishlamay qoldi</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Iltimos bizga fikr-mulohaza yuboring.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Iltimos bizga fikr-mulohaza yuborish uchun bu bildirishnomani bosing. Bu muammoni tushunishimizga yordam beradi va uni tezroq hal qilishimiz mumkin.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Men roziman</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Ma\'lumot yig\'ish haqida ogohlantirish</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s sizga internet kontentiga senzurasiz kirishni ta\'minlash uchun Android VPN xizmatidan foydalanadi. Biz quyidagi ma\'lumotlarni yig\'amiz, birlashtitramiz va saqlaymiz:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Sizning shahringiz, mamlakatingiz, ISP va ixtiyoriy ravishda, taxminiy joylashuvingiz. Shahar, mamlakat va ISP sizning serverlarimizga ulanayotgan IP manzilidan olinadi, IP manzilning o\'zi hech qachon saqlanmaydi. Bu ma\'lumotlar foydalanish va ishlash statistikasi hamda xizmat sog\'lig\'i maqsadlarida yig\'iladi.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Bizga diagnostika yoki ishlamay qolish jurnali fikr-mulohazasini yuborganingizda elektron pochtangiz. Fikr-mulohazada elektron pochta ko\'rsatish majburiy emas.</string>
  <!--Data collection disclosure list item-->
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_title">%s bildirish</string>
  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s ilova orqa fonda bo\'lganda sizga muhim ma\'lumotlar berish uchun bildirishnomalardan foydalanadi. Bu quyidagilarni o\'z ichiga oladi:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">VPN ulanish holatingizdagi o\'zgarishlar.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Ulanishingiz bilan bog\'liq muammolar.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Ishlamay qolish hisobotlari.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware ogohlantirish.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">Yangi ilova versiyalari mavjud.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">Bildirishnomalarni ilova sozlamalarida istalgan vaqtda o\'chirishingiz mumkin.</string>
