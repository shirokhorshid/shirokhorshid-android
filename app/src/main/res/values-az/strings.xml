  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s tətbiq arxa planda olarkən sizə vacib məlumatlar təqdim etmək üçün bildirişlərdən istifadə edir. Bu bunları əhatə edir:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">VPN bağlantı vəziyyətinizdəki dəyişikliklər.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Bağlantınızla bağlı problemlər.</string>
  <!--Notifications permission rationale list item-->
