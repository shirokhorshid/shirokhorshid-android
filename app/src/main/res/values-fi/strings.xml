  <!--Alert dialog message prompting the user to enable NFC in system settings, do not translate or transliterate 'Psiphon Bump'-->
  <string name="psiphon_bump_need_system_nfc_message">Ota käyttöön NFC järjestelmäasetuksissa ottaaksesi käyttöön Psiphon Bump apumoodi.</string>
  <!--Button that opens NFC related system settings-->
  <string name="psiphon_bump_need_system_nfc_open_btn">Avaa asetukset</string>
