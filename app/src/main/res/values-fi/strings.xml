  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Haittaohjelmaliikenne havaittu! Klikkaa saadaksesi lisätietoja.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon on havainnut ja estänyt pääsyn mahdollisesti haitallisille sivustoille internetissä. Seuraa alla olevia linkkejä saadaksesi lisätietoja.

Havaittuja haittaohjelmia:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon estää automaattisesti pääsyn mahdollisesti haitallisille sivustoille internetissä. Haluatko saada ilmoituksen, kun tämä tapahtuu?

Tätä asetusta voi muuttaa myöhemmin Lisäasetuksissa.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Lue lisää MalAware-ominaisuudesta:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Ylävirta proksivirhe</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Avaa Psiphon ja tarkista proksi-asetukset.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Proksivirhe</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Olet määrittänyt Psiphonin käyttämään ylävirta proksipalvelinta. Emme kuitenkaan pysty yhdistämään Psiphon-palvelimeen tämän proksipalvelimen kautta.

Korjaa asetukset ja yritä uudelleen.

Katso lokeista teknisiä tietoja.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon kaatui</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Lähetä meille palautetta.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Napsauta tätä ilmoitusta lähettääksesi meille palautetta. Tämä auttaa meitä ymmärtämään ongelman, jotta voimme korjata sen nopeammin.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Hyväksyn</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Tietojenkeruun ilmoitus</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s käyttää Android VPN -palvelua tarjotakseen sinulle sensuroimattoman pääsyn internet-sisältöön. Keräämme, yhdistämme ja tallennamme seuraavia tietoja:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Kaupunkisi, maasi, ISP ja valinnaisesti likimääräinen sijaintisi. Kaupunki, maa ja ISP johdetaan IP-osoitteesta, josta muodostat yhteyden palvelimiimme, eikä itse IP-osoitetta koskaan tallenneta. Näitä tietoja kerätään käyttö- ja suorituskykytilastoja ja palvelun terveyttä varten.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Sähköpostiosoitteesi, kun lähetät meille diagnostiikka- tai kaatumislokin palautetta. Sähköpostin antaminen palautteessa on valinnaista.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp3">PsiCash-tilisi käyttäjätunnus, salasana ja valinnaisesti sähköposti.</string>
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_title">%s Ilmoitukset</string>
  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s käyttää ilmoituksia tarjotakseen sinulle tärkeitä tietoja, kun sovellus on taustalla. Tämä sisältää:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">Muutokset VPN-yhteytesi tilassa.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Ongelmat yhteydessäsi.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Kaatumisraportit.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware-hälytykset.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">Uusia sovellusversioita saatavilla.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">Voit poistaa ilmoitukset käytöstä sovelluksen asetuksista milloin tahansa.</string>
