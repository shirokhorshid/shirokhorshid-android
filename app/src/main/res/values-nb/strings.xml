  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Skadelig programvaretrafikk oppdaget! Klikk for å lære mer.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon har oppdaget og blokkert tilgang til potensielt skadelige nettsteder på Internett. Følg lenkene nedenfor for å lære mer.
  Oppdaget skadelig programvare:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon blokkerer automatisk tilgang til potensielt skadelige nettsteder på Internett. Vil du bli varslet når dette skjer?
  Denne innstillingen kan endres senere i Flere alternativer.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Lær mer om MalAware her:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Oppstrøms proxy-feil</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Vennligst åpne Psiphon og sjekk proxy-innstillinger.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Proxy-feil</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Du har konfigurert Psiphon til å bruke en oppstrøms proxy. Vi ser imidlertid ut til å ikke kunne koble til en Psiphon-server gjennom den proxyen.
  Vennligst rett innstillingene og prøv igjen.
  Se logger for teknisk informasjon.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon krasjet</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Vennligst send oss tilbakemelding.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Vennligst klikk på dette varselet for å sende oss tilbakemelding. Dette vil hjelpe oss å forstå problemet slik at vi kan fikse det raskere.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Jeg godtar</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Datainnsamling oppløsning</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s bruker Android VPN-tjenesten for å gi deg usensurert tilgang til Internett-innhold. Vi samler inn, aggregerer og lagrer følgende informasjon:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Din by, land, ISP og valgfritt omtrentlig plassering. Byen, landet og ISP-en er avledet fra IP-adressen du kobler til våre servere fra, og IP-adressen selv blir aldri logget. Disse dataene samles inn for bruk og ytelsesstatistikk og tjenestehelsformål.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Din e-post når du sender oss diagnostikk eller krasjlogg tilbakemelding. Å oppgi e-post i tilbakemeldingen er valgfritt.</string>
  <!--Data collection disclosure list item-->
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_title">%s Varsler</string>
  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s bruker varsler for å gi deg viktig informasjon når appen er i bakgrunnen. Dette inkluderer:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">Endringer i din VPN-tilkoblingstilstand.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Problemer med tilkoblingen din.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Krasjrapporter.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware-varsler.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">Nye appversjoner tilgjengelig.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">Du kan deaktivere varsler i appinnstillingene når som helst.</string>
