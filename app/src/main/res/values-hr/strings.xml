  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Malware promet otkriven! Kliknite da saznate više.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon je otkrio i blokirao pristup potencijalno zlonamjernim web mjestima na internetu. Molimo pratite linkove ispod da saznate više.\n\nOtkriveni malware:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon automatski blokira pristup potencijalno zlonamjernim web mjestima na internetu. Želite li biti obaviješteni kada se to dogodi?\n\nOva postavka se može promijeniti kasnije u Više opcija.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Saznajte više o MalAware ovdje:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Greška uzvodnog proksija</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Molimo otvorite Psiphon i provjerite postavke proksija.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Greška proksija</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Konfigurirali ste Psiphon da koristi uzvodni proksi. Međutim, čini se da se ne možemo spojiti na Psiphon poslužitelj kroz taj proksi.\n\nMolimo popravite postavke i pokušajte ponovno.\n\nPogledajte zapisnike za tehničke informacije.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon se srušio</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Molimo pošaljite nam povratne informacije.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Molimo kliknite ovu obavijest da nam pošaljete povratne informacije. To će nam pomoći razumjeti problem kako bismo ga brže popravili.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Prihvaćam</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Objavljivanje prikupljanja podataka</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s koristi Android VPN uslugu za pružanje necenzuriranog pristupa internetskom sadržaju. Prikupljamo, agregiramo i pohranjujemo sljedeće informacije:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Vaš grad, zemlja, ISP i opcionalno, približna lokacija. Grad, zemlja i ISP se izvode iz IP adrese s koje se spajate na naše servere, a sama IP adresa se nikad ne zapisuje. Ovi podaci se prikupljaju za statistike korištenja i performansi te svrhe zdravlja usluge.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Vaš email kada nam šaljete dijagnostičke ili crash log povratne informacije. Pružanje emaila u povratnim informacijama je opcionalno.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp3">Vaše PsiCash korisničko ime, lozinka i opcionalno, email.</string>
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_title">%s Obavijesti</string>
  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s koristi obavijesti za pružanje važnih informacija kada je aplikacija u pozadini. To uključuje:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">Promjene u stanju vaše VPN veze.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Problemi s vašom vezom.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Izvještaji o rušenju.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware upozorenja.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">Dostupne nove verzije aplikacije.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">Možete onemogućiti obavijesti u postavkama aplikacije u bilo koje vrijeme.</string>
