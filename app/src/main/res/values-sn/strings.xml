  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Traffic yemaware yakawanikwa! Dzvanya kuti udzidze zvakawanda.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon yakaona nekuvhara kupinda kumasites angangoita kuti ukuvadze paInternet. Tapota tevera manzwiro ari pazasi kuti udzidze zvakawanda.\n\nMaware yakawanikwa:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon inongo vhara kupinda kumasites angangoita kuti ukuvadze paInternet. Unoda here kuziviswa kana izvi zvichiitika?\n\nIyi setting inogona kushandurwa gare gare muMore Options.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Dzidza zvakawanda nezveMalAware pano:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Kukanganisa kweproxy yekumusoro</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Tapota vhura Psiphon uye utarisa proxy settings.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Kukanganisa kweProxy</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Wakagadzirisa Psiphon kuti ishandise proxy yekumusoro. Nekudaro, tinotarisira kuti hatigonikone kubatana neserver yePsiphon kuburikidza neproxy iyoyo.\n\nTapota gadzirisa masettings uye uedze zvakare.\n\nOna matarwa kuziva tekiniki mamiriro.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon Yakaputsika</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Tapota titumire mhinduro.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Tapota dzvanya chiziviso ichi kuti utitumire mhinduro. Izvi zvichatabatsira kuti tinzwisise dambudziko kuti tigone kurigadzirisa nokukasira.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Ndinobvuma</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Kuburitsira pakunze nekuunganidzira data</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s inoshandisa basa reAndroid VPN kukupa iwe kupinda kune zviri muInternet zvisina kudziviswa. Tinounganidza, tinobatanidza, uye tinochengetera ruzivo runotevera:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Guta rako, nyika, ISP, uye zvingangodaro, nzvimbo inofungidzirwa. Guta, nyika, uye ISP zvinobva kuIP address yaunobatana nawo kuserver dzedu, uye IP address pachayo haina kumbotungamirwa. Data iyi inounganidza nokuda kwekuwanikwa nekushanda kwezviratidzo nekuda kwehutano hwesevisi.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Email yako, kana uchititumira diagnostics kana crash log feedback. Kupa email mumhinduro hazvimanikidzwi.</string>
  <!--Data collection disclosure list item-->
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
