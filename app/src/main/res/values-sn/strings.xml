  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s inoshandisa zviziviso kukupa iwe ruzivo rukakosha app yapo irikumashure. Izvi zvinosanganisira:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">Shanduko kumamiriro ako eVPN kubatana.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Matambudziko nekubatana kwako.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Mishumo yekuputsika.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware alerts.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">App mavhezheni matsva anowanikwa.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">Unogona kuisa zviziviso kuti zvisashande muapp settings chero nguva.</string>
